{
    "name": "rennokki/elasticscout",
    "type": "library",
    "description": "Elasticsearch driver for Laravel Scout.",
    "keywords": [
        "elastic", "elasticsearch", "driver", "engine", "laravel", "scout", "search"
    ],
    "homepage": "https://github.com/rennokki/elasticscout",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "elasticsearch/elasticsearch": ">=7.0 <7.2",
        "laravel/scout": "^7.2",
        "aws/aws-sdk-php": "^3.132"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\ElasticScout\\Tests\\": "tests/",
            "Rennokki\\ElasticScout\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\ElasticScout\\ElasticScoutServiceProvider"
            ]
        }
    }
}
